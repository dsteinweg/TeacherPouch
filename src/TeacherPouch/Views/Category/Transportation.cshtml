        <div class="col-sm-6 col-md-4">
            <a href="@Url.Search("car")"><img src="@Url.VersionedContent("~/img/automobilesS.jpg")" class="img-responsive" alt="Automobiles"></a>
        </div>

        <div class="col-sm-6 col-md-4">
            <a href="@Url.Search("boat")"><img src="@Url.VersionedContent("~/img/boatsS.jpg")" class="img-responsive" alt="Boats"></a>
        </div>

        <div class="col-sm-6 col-md-4">
            <a href="@Url.Search("walk")"><img src="@Url.VersionedContent("~/img/walkS.jpg")" class="img-responsive" alt="Walk"></a>
