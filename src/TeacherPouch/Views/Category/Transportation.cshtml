@{
    ViewBag.Title = "Transportation - Category";
}

<div class="category">
    <header>
        <div class="jumbotron">
            <img src="@Url.VersionedContent("~/img/transportationS.jpg")" class="img-responsive center-block" alt="Transportation">
            <h3>Zoom.... Zoom...</h3>
        </div>
    </header>

    <div class="row featured-tags-preview">
        <div class="col-sm-6 col-md-4">
            <a href="@Url.Search("planes")"><img src="@Url.VersionedContent("~/img/planesS.jpg")" class="img-responsive" alt="Planes"></a>
